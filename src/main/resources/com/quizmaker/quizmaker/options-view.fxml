<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="561.0" prefWidth="600.0" style="-fx-background-color: #00008B;" stylesheets="@css/style.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.quizmaker.quizmaker.controller.OptionsController">
   <children>
      <Text fill="WHITE" layoutX="201.0" layoutY="79.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Options">
         <font>
            <Font name="Lucida Console" size="47.0" />
         </font>
      </Text>
      <Button fx:id="audioBtn" layoutX="305.0" layoutY="144.0" mnemonicParsing="false" onAction="#audioControl" prefHeight="92.0" prefWidth="253.0" text="Audio">
         <font>
            <Font name="Lucida Console" size="38.0" />
         </font>
      </Button>
      <Button fx:id="controlBtn" layoutX="305.0" layoutY="254.0" mnemonicParsing="false" onAction="#showControl" prefHeight="92.0" prefWidth="253.0" text="Controls">
         <font>
            <Font name="Lucida Console" size="38.0" />
         </font>
      </Button>
      <Button fx:id="resetBtn" layoutX="305.0" layoutY="367.0" mnemonicParsing="false" onAction="#resetGame" prefHeight="92.0" prefWidth="253.0" text="Reset">
         <font>
            <Font name="Lucida Console" size="38.0" />
         </font>
      </Button>
      <Button fx:id="backBtn" layoutX="21.0" layoutY="511.0" mnemonicParsing="false" onAction="#backToMenu" prefHeight="32.0" prefWidth="71.0" text="Back">
